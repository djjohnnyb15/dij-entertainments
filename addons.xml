<?xml version="1.0" encoding="UTF-8"?>
<addons>
<addon id="plugin.video.Dijentertainments"
       name="Dijentertainments"
       version="1.0"
       provider-name="coder">
  <requires>
<import addon="script.module.simplejson" version="3.3.0"/>
<import addon="xbmc.python" version="2.1.0"/>
<import addon="script.module.t0mm0.common" version="2.0.0"/>
<import addon="plugin.video.youtube" version="3.0.0"/>
<import addon="script.module.urlresolver" version="2.1.2"/>
<import addon="script.common.plugin.cache" version="2.5.2"/>
<import addon="script.module.metahandler" version="2.5.1"/>
  </requires>
  <extension point="xbmc.python.pluginsource"
            library="default.py">
        <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="en">all you want in 1 plugin</summary>
    <description lang="en">Movies ,Tv and XXX</description>
    <platform>all</platform>
  </extension>
</addon>

<addon id="repository.Dijentertainments" name="repository.Dijentertainments" version="1.0" provider-name="coder">
	<extension point="xbmc.addon.repository" name="YourRepo Addon Repository">
		<info compressed="false">http://raw.github.com/djjohnnyb15/dij-entertainments/master/addons.xml</info>
		<checksum>http://raw.github.com/djjohnnyb15/dij-entertainments/master/addons.xml.md5</checksum>
		<datadir zip="true">http://raw.github.com/djjohnnyb15/dij-entertainments/master/zips</datadir>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary></summary>
		<description></description>
		<platform>all</platform>
	</extension>
</addon>
</addons>
